<script>
  document.addEventListener('DOMContentLoaded', () => {
    // Search functionality
    const searchInput = document.getElementById('stateSearch');
    const stateCards = document.querySelectorAll('.state-card');
    
    searchInput?.addEventListener('input', (e) => {
      const searchTerm = e.target.value.toLowerCase();
      
      stateCards.forEach(card => {
        const stateName = card.querySelector('h3').textContent.toLowerCase();
        const cityLinks = card.querySelectorAll('a');
        let hasMatchingCity = false;
        
        cityLinks.forEach(link => {
          if (link.textContent.toLowerCase().includes(searchTerm)) {
            hasMatchingCity = true;
          }
        });
        
        if (stateName.includes(searchTerm) || hasMatchingCity) {
          card.style.display = 'block';
        } else {
          card.style.display = 'none';
        }
      });
    });
    
    // Toggle functionality for cities
    const toggleButtons = document.querySelectorAll('.toggle-cities');
    
    toggleButtons.forEach(button => {
      button.addEventListener('click', () => {
        const citiesList = button.closest('.cities-list');
        const hiddenCities = citiesList.querySelector('.cities-hidden');
        const isHidden = hiddenCities.classList.contains('hidden');
        
        if (isHidden) {
          hiddenCities.classList.remove('hidden');
          button.textContent = button.textContent.replace('+', '‚àí');
        } else {
          hiddenCities.classList.add('hidden');
          button.textContent = button.textContent.replace('‚àí', '+');
        }
      });
    });
  });
</script>